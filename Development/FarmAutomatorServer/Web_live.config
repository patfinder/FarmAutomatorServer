  <system.web>
    <globalization culture="en-US" uiCulture="en-US"/>
    <authentication mode="Forms">
      <!--loginUrl="Home/Login"-->
      <forms name=".ASPXFORMSDEMO__" protection="All" path="/" timeout="15" slidingExpiration="true" />
    </authentication>

    <compilation debug="true" targetFramework="4.0" />
    <httpRuntime targetFramework="4.0" />
  </system.web>
  
  <!-- =========================== log4net =========================== -->
  <log4net>
    <!-- A1 is set to be a ConsoleAppender -->
    <!--${AppData} log to Windows %AppData% folder-->
    <appender name="FileAppender1" type="log4net.Appender.RollingFileAppender">
      <!--<file value="${AppData}\VisitmeServer.log" />-->
      <!--<file value="d:\Projects\VM\VisitMeServer\VisitmeServer.log" />-->
      <file value="D:\__Websites\Saleman\App_Data\Logs\SaleManager.log" />
      <appendToFile value="true" />
      <staticLogFileName value="true" />
      <rollingStyle value="Size" />
      <countDirection value="-1" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <!-- Set root logger level to DEBUG and its only appender to A1 -->
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender1" />
    </root>
  </log4net>
    
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  
    <modules>
